import matplotlib.pyplot as plt
n = [12, 27, 3, 19, 34, 8, 21, 5, 30, 16,
 2, 25, 11, 35, 7, 18, 29, 14, 9, 22,
 4, 31, 17, 6, 28, 13, 24, 1, 20, 33]
x = list(range(1, len(n) + 1))
plt.plot(x, n, marker='o')
plt.title("Line Graph of Given Numbers")
plt.xlabel("Index Number")
plt.ylabel("Values")
plt.grid()
plt.show()
